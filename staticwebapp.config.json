{
    "routes": [
      {
        "route": "/studio/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/static/*", "/images/*", "/favicon.ico", "/studio/api/*"]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    }
  }
  